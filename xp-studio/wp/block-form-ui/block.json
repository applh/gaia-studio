{
    "apiVersion": 3,
    "name": "xps/form-ui",
    "title": "XPS: Form UI",
    "category": "text",
    "icon": "universal-access-alt",
    "editorScript": "file:./block.js",
    "editorStyle": "file:./editor.css",
    "style": "file:./front.css",
    "attributes":{
        "ui_type": {
            "type": "string",
            "default": "text"
        },
        "ui_label": {
            "type": "string",
            "default": "label"
        },
        "ui_name": {
            "type": "string",
            "default": "name"
        },
        "ui_html": {
            "type": "string",
            "default": ""
        }
    },
    "supports": {
        "extraClassname": true,
        "align": true,
        "alignWide": true,
        "color": {
            "gradients": true,
            "background": true,
            "text": true,
            "link": true
        },
        "dimensions": {
            "minHeight": true
        },
        "position":{
            "sticky": true
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "blockGap": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true
        }
    }
}